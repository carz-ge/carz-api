type Product {
    id: String!
    name: String!
}

type Catalog {
    id: String!
    name: String!
    products: [Product]
}

extend type Query {
    getCatalogs: [Catalog]!
    getProducts: [Product]!
}

input ProductInput {
    name: String!
}

extend type Mutation {
    addProduct(input: ProductInput!): Product!
}

